			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="xunit" Version="2.9.3" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\DbSqlLikeMem.MySql\DbSqlLikeMem.MySql.csproj" AdditionalProperties="TargetFramework=$(TargetFramework)" />
		<ProjectReference Include="..\DbSqlLikeMem.Test\DbSqlLikeMem.Test.netstandard20.csproj" AdditionalProperties="TargetFramework=$(TargetFramework)" />
	</ItemGroup>

	<ItemGroup>
		<Using Include="Dapper" />
		<Using Include="DbSqlLikeMem" />
		<Using Include="DbSqlLikeMem.MySql" />
		<Using Include="DbSqlLikeMem.Test" />
		<Using Include="FluentAssertions" />
		<Using Include="MySql.Data.MySqlClient" />
		<Using Include="System.Data" />
		<Using Include="System.Globalization" />
		<Using Include="Xunit.Abstractions" />
		<Using Include="Xunit" />
	</ItemGroup>

</Project>